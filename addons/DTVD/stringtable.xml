    <Package name="Options">
        <Key ID="STR_DTVD_enableOption">
            <English>Enable View Distance Management</English>
            <German>Aktiviere Sichtweiten Manager</German>
            <Japanese>描画視程設定を有効化</Japanese>
            <Turkish>Görüş Mesafesi Düzenleyicisini Etkinleştir</Turkish>
        </Key>
        <Key ID="STR_DTVD_enableOptionDescription">
            <English>Allow the DT View Distance Manager to manage your view distance and terrain grid.</English>
            <German>Erlaube den DT Sichtweiten Manager die Sichtweite und Geländeraster einzustellen.</German>
            <Japanese>DTVDで描画視程と地形品質を管理できるようにします。</Japanese>
            <Turkish>DT Görüş Mesafesi Düzenleyicisine görüş mesafesini ve araziyi ayarlaması için izin ver.</Turkish>
        </Key>
        <Key ID="STR_DTVD_focusedBoost">
            <English>Enable Focused Zoom Boost</English>
            <Japanese>注視時のズームブーストを有効化</Japanese>
        </Key>
        <Key ID="STR_DTVD_focusedBoostDescription">
            <English>When you focus your view (hold right click etc), your current view and object view distance is boosted by the configured value.</English>
            <Japanese>この設定を有効化すると、注視時(右クリックを長押しした時に"フォーカス"された時)に現在の(総合/オブジェクト)描画距離が設定値の分だけブーストされるようになります。</Japanese>
        </Key>
        <Key ID="STR_DTVD_hotkey1">
            <English>View distance hotkey 1</English>
            <Japanese>視程距離ホットキー 1</Japanese>
        </Key>
        <Key ID="STR_DTVD_hotkey2">
            <English>View distance hotkey 2</English>
            <Japanese>視程距離ホットキー 2</Japanese>
        </Key>
        <Key ID="STR_DTVD_hotkey3">
            <English>View distance hotkey 3</English>
            <Japanese>視程距離ホットキー 3</Japanese>
        </Key>
        <Key ID="STR_DTVD_incrementDistance">
            <English>Distance Increment Value</English>
            <Japanese>距離の増減値</Japanese>
        </Key>
        <Key ID="STR_DTVD_incrementDistanceDescription">
            <English>Select the distance you want added or subtracted when the selected hotkey is pressed.</English>
            <Japanese>選択したホットキーを押したときに増加もしくは減少する距離を設定します。</Japanese>
        </Key>
        <Key ID="STR_DTVD_incrementHotkeyAdd">
            <English>Increment distance hotkey</English>
            <Japanese>距離増加ホットキー</Japanese>
        </Key>
        <Key ID="STR_DTVD_incrementHotkeyRemove">
            <English>Decrement distance hotkey</English>
            <Japanese>距離減少ホットキー</Japanese>
        </Key>
        <Key ID="STR_DTVD_incrementedViewHotkey">
            <English>View distance changed: %1m (%2%3m).</English>
            <Japanese>描画距離が変更されました: %1m (%2%3m)</Japanese>
        </Key>
        <Key ID="STR_DTVD_maxDistance">
            <English>Max View Distance</English>
            <German>Maximale Sichtweite</German>
            <Japanese>最大描画視程距離</Japanese>
            <Turkish>Azami Görüş Mesafesi</Turkish>
        </Key>
        <Key ID="STR_DTVD_maxDistanceDescription">
            <English>Select the max view distance players may use.</English>
            <German>Einstellung der maximalen Sichtweite des Spieler.</German>
            <Japanese>プレイヤーが使用できる最大描画視程距離を指定します。</Japanese>
            <Turkish>Oyuncuların kullanabileceği azami görüş mesafesini seç.</Turkish>
        </Key>
        <Key ID="STR_DTVD_maxTerrainGrid">
            <English>Max Terrain Grid</English>
            <Japanese>最大地形グリッド</Japanese>
        </Key>
        <Key ID="STR_DTVD_maxTerrainGridDescription">
            <English>Select the max terrain grid players may use.</English>
            <Japanese>プレイヤーが使用できる地形グリッドの最高値(高い=処理優先)を指定します。</Japanese>
        </Key>
        <Key ID="STR_DTVD_modSubCategory">
            <English>View Distance</English>
            <German>Sichtweite</German>
            <Japanese>視程距離</Japanese>
            <Turkish>Görüş Mesafesi</Turkish>
        </Key>
        <Key ID="STR_DTVD_notifications">
            <English>Enable Notifications</English>
            <Japanese>通知を有効化</Japanese>
        </Key>
        <Key ID="STR_DTVD_notificationsDescription">
            <English>Enable notifications when you change the view distance by a hotkey.</English>
            <Japanese>ホットキーの入力で描画距離が変更された際に通知を表示する機能を有効化します</Japanese>
        </Key>
        <Key ID="STR_DTVD_objectSync">
            <English>Automatically sync object distance</English>
            <Japanese>描画距離の自動同期</Japanese>
        </Key>
        <Key ID="STR_DTVD_objectSyncDescription">
            <English>Automatically sync changes to the object view distance slider to match the selected terrain view distance slider.</English>
            <Japanese>設定した総合視程と一致するように、オブジェクトの描画視程の変更を自動的に同期します。</Japanese>
        </Key>
        <Key ID="STR_DTVD_openMenu">
            <English>Open view distance menu</English>
            <German>Öffne Sichtweitenmenü</German>
            <Japanese>描画視程設定を開く</Japanese>
            <Turkish>Görüş mesafesi menüsünü aç</Turkish>
        </Key>
        <Key ID="STR_DTVD_presetViewHotkey">
            <English>View distance changed: %1m (Hotkey %2).</English>
            <Japanese>描画距離が変更されました: %1m (ホットキー %2)</Japanese>
        </Key>
        <Key ID="STR_DTVD_scopeBoost">
            <English>Enable Scope Zoom Boost</English>
            <Japanese>照準器使用時のズームブーストを有効化</Japanese>
        </Key>
        <Key ID="STR_DTVD_scopeBoostDescription">
            <English>When you zoom in through a scope, your current view and object view distance is boosted by the configured value.</English>
            <Japanese>この設定を有効化すると、スコープなどの照準器を覗き見た時に現在の(総合/オブジェクト)描画距離が設定値の分だけブーストされるようになります。</Japanese>
        </Key>
        <Key ID="STR_DTVD_viewDistanceHotkey1">
            <English>Change view hotkey 1</English>
            <Japanese>ホットキー1の設定距離</Japanese>
        </Key>
        <Key ID="STR_DTVD_viewDistanceHotkey2">
            <English>Change view hotkey 2</English>
            <Japanese>ホットキー2の設定距離</Japanese>
        </Key>
        <Key ID="STR_DTVD_viewDistanceHotkey3">
            <English>Change view hotkey 3</English>
            <Japanese>ホットキー3の設定距離</Japanese>
        </Key>
        <Key ID="STR_DTVD_viewDistanceHotkeyDescription">
            <English>Select the view and object distance you would like to be changed to when pressing the selected hotkey.</English>
            <Japanese>選択したホットキーを押したときに変更される総合描画とオブジェクト描画の視程距離を設定します。</Japanese>
        </Key>
